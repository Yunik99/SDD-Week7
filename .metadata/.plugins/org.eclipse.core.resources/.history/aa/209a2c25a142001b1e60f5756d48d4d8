
public class Sculpture extends ArtWord {
	String material;
	int weight;
	
	public Sculpture(String artishName, int valueInPound, String material, int weight) {
		super(artishName, valueInPound);
		this.material = material;
		this.weight = weight;
	}
	
	public String getMaterial() {
		return material;
	}
	public void setMaterial(String material) {
		this.material = material;
	}
	public int getWeight() {
		return weight;
	}
	public void setWeight(int weight) {
		this.weight = weight;
	}
	public void className() {
		System.out.println("This is a Sculpture class.");
	}
	public void callSuperClass() {
		System.out.println("We are calling super class from Sculpture class.");
		
	}
}
