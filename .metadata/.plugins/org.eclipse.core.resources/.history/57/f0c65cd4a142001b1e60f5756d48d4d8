
public class program {

	public static void main(String[] args) {
		
		
		Sculpture jeus = new Sculpture("JEUS", 99, "GOLD", 600);
		Painting mona = new Painting("PISA", 50, "Color", 10, 10, 2);
		
		System.out.println(jeus.getArtishName());
		System.out.println(jeus.getValueInPound());
		System.out.println(jeus.getMaterial());
		System.out.println(jeus.getWeight());
		
		System.out.println(mona.getArtishName());
		System.out.println(mona.getValueInPound());
		System.out.println(mona.getPaintType());
		System.out.println(mona.getWidth());
		System.out.println(mona.getHeight());
		System.out.println(mona.getWeight());		
	}
}
